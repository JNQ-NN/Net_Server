#mainLog
log4cplus.logger.mainLog = info, console
log4cplus.appender.console = log4cplus::ConsoleAppender
log4cplus.appender.console.layout = log4cplus::PatternLayout
log4cplus.appender.console.layout.ConversionPattern = %D{%m/%d/%y %H:%M:%S} [%-5p][%l] %m%n

#consoleLog
log4cplus.logger.consoleLog = ALL, console
log4cplus.appender.console = log4cplus::ConsoleAppender
log4cplus.appender.console.layout = log4cplus::PatternLayout
log4cplus.appender.console.layout.ConversionPattern = %D{%m/%d/%y %H:%M:%S} [%-5p][%l] %m%n
